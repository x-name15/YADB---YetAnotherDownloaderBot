DISCORD_TOKEN=token
BOT_PREFIX=+
BOT_NAME=MediaDownloader
BOT_VERSION=1.0.0

# Rich Presence
RPC_ENABLED=true
RPC_STATE=Descargando contenido
RPC_DETAILS=Ayudando a usuarios
RPC_LARGE_IMAGE=bot_logo
RPC_LARGE_TEXT=Bot de Descarga de Medios
RPC_SMALL_IMAGE=discord_logo
RPC_SMALL_TEXT=Powered by Discord.py

# MongoDB
MONGODB_ENABLED=true
MONGODB_URI=mongodb://mongo:27017/
MONGODB_DB=mediadownloader
MONGODB_USER=
MONGODB_PASSWORD=
MONGODB_AUTH_SOURCE=admin

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0

# limits
MAX_DOWNLOADS=4
# Timeout
DOWNLOAD_TIMEOUT=600
